{
	"name": "application",
	"version": "1.0.0",
	"description": "Meteor Discord application.",
	"type": "module",
	"main": "dist/index.js",
	"scripts": {
		"start": "node --env-file=.env .",
		"build": "tsc",
		"format": "biome format --write .",
		"lint": "biome lint --write .",
		"check": "biome check --write .",
		"typecheck": "tsc --noEmit"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/meteor-discord/application.git"
	},
	"author": "Meteor Labs",
	"license": "CC BY-NC-SA 4.0",
	"packageManager": "pnpm@10.23.0",
	"engines": {
		"node": ">=22",
		"pnpm": ">=10"
	},
	"dependencies": {
		"discord.js": "^14.25.1"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.12",
		"@types/node": "^25.0.10",
		"typescript": "^5.9.3"
	}
}
